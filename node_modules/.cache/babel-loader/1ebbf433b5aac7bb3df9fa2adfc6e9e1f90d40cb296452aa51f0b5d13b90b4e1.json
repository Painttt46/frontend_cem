{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'LeaveApproval',\n  props: {\n    records: Array\n  },\n  created() {\n    this.$http = axios.create({\n      baseURL: process.env.VUE_APP_API_URL || ''\n    });\n  },\n  data() {\n    return {\n      showWorkDetailsDialog: false,\n      selectedWorkDetails: '',\n      showAttachmentsDialog: false,\n      selectedAttachments: [],\n      leaveTypes: []\n    };\n  },\n  async mounted() {\n    await this.loadLeaveTypes();\n  },\n  methods: {\n    async loadLeaveTypes() {\n      try {\n        const response = await this.$http.get('/api/leave/leave-types');\n        this.leaveTypes = response.data;\n        console.log('Leave types loaded:', this.leaveTypes);\n      } catch (error) {\n        console.error('Error loading leave types:', error);\n      }\n    },\n    getLeaveTypeColor(type) {\n      const leaveType = this.leaveTypes.find(lt => lt.value === type);\n      const color = leaveType?.color || '#6c757d';\n      console.log('Getting color for type:', type, 'found:', leaveType, 'color:', color);\n      return color;\n    },\n    showWorkDetails(workDetails) {\n      this.selectedWorkDetails = workDetails;\n      this.showWorkDetailsDialog = true;\n    },\n    showAttachments(attachments) {\n      this.selectedAttachments = attachments || [];\n      this.showAttachmentsDialog = true;\n    },\n    downloadFile(fileName) {\n      this.$toast.add({\n        severity: 'success',\n        summary: 'ดาวน์โหลด',\n        detail: `กำลังดาวน์โหลด: ${fileName}`,\n        life: 3000\n      });\n    },\n    getFileIcon(fileName) {\n      const extension = fileName.split('.').pop()?.toLowerCase();\n      const icons = {\n        pdf: 'pi pi-file-pdf',\n        doc: 'pi pi-file-word',\n        docx: 'pi pi-file-word',\n        xls: 'pi pi-file-excel',\n        xlsx: 'pi pi-file-excel',\n        jpg: 'pi pi-image',\n        jpeg: 'pi pi-image',\n        png: 'pi pi-image',\n        gif: 'pi pi-image'\n      };\n      return icons[extension] || 'pi pi-file';\n    },\n    getFileType(fileName) {\n      const extension = fileName.split('.').pop()?.toLowerCase();\n      const types = {\n        pdf: 'PDF Document',\n        doc: 'Word Document',\n        docx: 'Word Document',\n        xls: 'Excel Spreadsheet',\n        xlsx: 'Excel Spreadsheet',\n        jpg: 'JPEG Image',\n        jpeg: 'JPEG Image',\n        png: 'PNG Image',\n        gif: 'GIF Image'\n      };\n      return types[extension] || 'Unknown File';\n    },\n    formatDateTime(datetime) {\n      if (!datetime) return '-';\n      return new Date(datetime).toLocaleString('th-TH', {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    },\n    formatDate(datetime) {\n      if (!datetime) return '-';\n      return new Date(datetime).toLocaleDateString('th-TH', {\n        day: '2-digit',\n        month: '2-digit'\n      });\n    },\n    calculateDays(startDateTime, endDateTime) {\n      if (!startDateTime || !endDateTime) return 0;\n      const start = new Date(startDateTime);\n      const end = new Date(endDateTime);\n      const diffTime = Math.abs(end - start);\n      return Math.ceil(diffTime / (1000 * 60 * 60 * 24)) + 1;\n    },\n    getLeaveTypeLabel(type) {\n      const types = {\n        sick: 'ลาป่วย',\n        personal: 'ลากิจ',\n        vacation: 'ลาพักร้อน',\n        maternity: 'ลาคลอด',\n        other: 'ลาอื่นๆ'\n      };\n      return types[type] || type;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","records","Array","created","$http","create","baseURL","process","env","VUE_APP_API_URL","data","showWorkDetailsDialog","selectedWorkDetails","showAttachmentsDialog","selectedAttachments","leaveTypes","mounted","loadLeaveTypes","methods","response","get","console","log","error","getLeaveTypeColor","type","leaveType","find","lt","value","color","showWorkDetails","workDetails","showAttachments","attachments","downloadFile","fileName","$toast","add","severity","summary","detail","life","getFileIcon","extension","split","pop","toLowerCase","icons","pdf","doc","docx","xls","xlsx","jpg","jpeg","png","gif","getFileType","types","formatDateTime","datetime","Date","toLocaleString","year","month","day","hour","minute","formatDate","toLocaleDateString","calculateDays","startDateTime","endDateTime","start","end","diffTime","Math","abs","ceil","getLeaveTypeLabel","sick","personal","vacation","maternity","other"],"sources":["D:\\GEN-T\\WEB\\GenT-CEM\\frontend_cem\\src\\views\\leave_work\\LeaveApproval.vue"],"sourcesContent":["<template>\r\n  <Card class=\"approval-card\">\r\n    <template #content>\r\n      <div v-if=\"records.length === 0\" class=\"empty-state\">\r\n        <i class=\"pi pi-check-circle\" style=\"font-size: 4rem; color: #ccc;\"></i>\r\n        <p>ไม่มีคำขอลางานที่รอการอนุมัติ</p>\r\n      </div>\r\n\r\n      <DataTable v-else :value=\"records\" :paginator=\"true\" :rows=\"10\"\r\n        :rowsPerPageOptions=\"[5, 10, 20]\" responsiveLayout=\"scroll\" class=\"approval-table\" stripedRows>\r\n        \r\n        <Column field=\"id\" header=\"Ticket ID\" :sortable=\"true\" style=\"min-width: 120px;\">\r\n          <template #body=\"slotProps\">\r\n            <div class=\"ticket-id-container\">\r\n              <Badge :value=\"`#${String(slotProps.data.id).padStart(4, '0')}`\" class=\"ticket-badge\" />\r\n            </div>\r\n          </template>\r\n        </Column>\r\n\r\n        <Column field=\"employee_name\" header=\"ชื่อ-นามสกุล\" :sortable=\"true\" />\r\n        <Column field=\"employee_position\" header=\"ตำแหน่ง\" :sortable=\"true\" />\r\n        \r\n        <Column field=\"leave_type\" header=\"ประเภทการลา\" :sortable=\"true\">\r\n          <template #body=\"slotProps\">\r\n            <Badge :value=\"getLeaveTypeLabel(slotProps.data.leave_type)\" :style=\"{ backgroundColor: getLeaveTypeColor(slotProps.data.leave_type), color: '#fff', fontWeight: 'bold' }\" />\r\n          </template>\r\n        </Column>\r\n\r\n        <Column header=\"วันเวลาเริ่มลา\" :sortable=\"true\">\r\n          <template #body=\"slotProps\">\r\n            {{ formatDateTime(slotProps.data.start_datetime) }}\r\n          </template>\r\n        </Column>\r\n\r\n        <Column header=\"วันเวลาสิ้นสุด\" :sortable=\"true\">\r\n          <template #body=\"slotProps\">\r\n            {{ formatDateTime(slotProps.data.end_datetime) }}\r\n          </template>\r\n        </Column>\r\n\r\n        <Column header=\"จำนวนวัน\">\r\n          <template #body=\"slotProps\">\r\n            {{ slotProps.data.total_days }} วัน\r\n          </template>\r\n        </Column>\r\n\r\n        <Column field=\"reason\" header=\"เหตุผล\" style=\"min-width: 160px; width: 160px;\">\r\n          <template #body=\"slotProps\">\r\n            <span class=\"reason-text-wrap\">{{ slotProps.data.reason || '-' }}</span>\r\n          </template>\r\n        </Column>\r\n\r\n        <Column header=\"ผู้รับผิดชอบแทน\" style=\"min-width: 200px;\">\r\n          <template #body=\"slotProps\">\r\n            <div v-if=\"slotProps.data.has_delegation\" class=\"delegate-info\">\r\n              <div class=\"delegate-name\">\r\n                <i class=\"pi pi-user\"></i>\r\n                <strong>{{ slotProps.data.delegate_name || 'ไม่ระบุ' }}</strong>\r\n              </div>\r\n              <div class=\"delegate-position\">\r\n                <i class=\"pi pi-briefcase\"></i>\r\n                <small>{{ slotProps.data.delegate_position || 'ไม่ระบุตำแหน่ง' }}</small>\r\n              </div>\r\n              <div class=\"delegate-department\">\r\n                <i class=\"pi pi-building\"></i>\r\n                <small>{{ slotProps.data.delegate_department || 'ไม่ระบุแผนก' }}</small>\r\n              </div>\r\n              <div class=\"delegate-contact\">\r\n                <i class=\"pi pi-phone\"></i>\r\n                <small class=\"contact-info\">{{ slotProps.data.delegate_contact || 'ไม่ระบุ' }}</small>\r\n              </div>\r\n              <div v-if=\"slotProps.data.work_details\" class=\"work-details-btn\">\r\n                <Button \r\n                  icon=\"pi pi-list\" \r\n                  label=\"ดูรายละเอียดงาน\" \r\n                  size=\"small\" \r\n                  severity=\"info\" \r\n                  outlined\r\n                  @click=\"showWorkDetails(slotProps.data.work_details)\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <span v-else class=\"no-delegation\">\r\n              <i class=\"pi pi-times-circle\"></i>\r\n              ไม่มีการมอบหมายงาน\r\n            </span>\r\n          </template>\r\n        </Column>\r\n\r\n        <Column header=\"เอกสารแนบ\" style=\"width: 80px;\">\r\n          <template #body=\"slotProps\">\r\n            <div v-if=\"slotProps.data.attachments && slotProps.data.attachments.length > 0\" class=\"attachments-info\">\r\n              <Button \r\n                icon=\"pi pi-paperclip\" \r\n                size=\"small\" \r\n                severity=\"info\" \r\n                outlined\r\n                @click=\"showAttachments(slotProps.data.attachments)\"\r\n                v-tooltip=\"`${slotProps.data.attachments.length} ไฟล์`\"\r\n              />\r\n            </div>\r\n            <span v-else>-</span>\r\n          </template>\r\n        </Column>\r\n\r\n        <Column header=\"วันที่ส่งคำขอ\" :sortable=\"true\">\r\n          <template #body=\"slotProps\">\r\n            {{ formatDateTime(slotProps.data.created_at) }}\r\n          </template>\r\n        </Column>\r\n\r\n        <Column header=\"การดำเนินการ\" :frozen=\"true\" alignFrozen=\"right\">\r\n          <template #body=\"slotProps\">\r\n            <div class=\"action-buttons\">\r\n              <Button \r\n                icon=\"pi pi-check\" \r\n                severity=\"success\" \r\n                size=\"small\"\r\n                @click=\"$emit('approve-leave', slotProps.data.id)\"\r\n                v-tooltip=\"'อนุมัติ'\"\r\n              />\r\n              <Button \r\n                icon=\"pi pi-times\" \r\n                severity=\"danger\" \r\n                size=\"small\"\r\n                @click=\"$emit('reject-leave', slotProps.data.id)\"\r\n                v-tooltip=\"'ไม่อนุมัติ'\"\r\n              />\r\n            </div>\r\n          </template>\r\n        </Column>\r\n      </DataTable>\r\n    </template>\r\n  </Card>\r\n\r\n  <!-- Work Details Dialog -->\r\n  <Dialog v-model:visible=\"showWorkDetailsDialog\" modal header=\"รายละเอียดงานที่มอบหมาย\" style=\"width: 50rem;\">\r\n    <div class=\"work-details-content\">\r\n      <p>{{ selectedWorkDetails }}</p>\r\n    </div>\r\n    <template #footer>\r\n      <Button label=\"ปิด\" icon=\"pi pi-times\" @click=\"showWorkDetailsDialog = false\" />\r\n    </template>\r\n  </Dialog>\r\n\r\n  <!-- Attachments Dialog -->\r\n  <Dialog v-model:visible=\"showAttachmentsDialog\" modal header=\"เอกสารแนบ\" style=\"width: 60rem;\">\r\n    <div class=\"attachments-content\">\r\n      <div v-if=\"selectedAttachments.length === 0\" class=\"no-attachments\">\r\n        <i class=\"pi pi-file\" style=\"font-size: 3rem; color: #ccc;\"></i>\r\n        <p>ไม่มีเอกสารแนบ</p>\r\n      </div>\r\n      <div v-else class=\"attachments-list\">\r\n        <div v-for=\"(file, index) in selectedAttachments\" :key=\"index\" class=\"attachment-item\">\r\n          <div class=\"file-info\">\r\n            <i :class=\"getFileIcon(file)\" class=\"file-icon\"></i>\r\n            <div class=\"file-details\">\r\n              <span class=\"file-name\">{{ file }}</span>\r\n              <small class=\"file-type\">{{ getFileType(file) }}</small>\r\n            </div>\r\n          </div>\r\n          <div class=\"file-actions\">\r\n            <Button \r\n              icon=\"pi pi-download\" \r\n              size=\"small\" \r\n              severity=\"success\" \r\n              outlined\r\n              @click=\"downloadFile(file)\"\r\n              v-tooltip=\"'ดาวน์โหลด'\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <template #footer>\r\n      <Button label=\"ปิด\" icon=\"pi pi-times\" @click=\"showAttachmentsDialog = false\" />\r\n    </template>\r\n  </Dialog>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\n\r\nexport default {\r\n  name: 'LeaveApproval',\r\n  props: {\r\n    records: Array\r\n  },\r\n  created() {\r\n    this.$http = axios.create({\r\n      baseURL: process.env.VUE_APP_API_URL || ''\r\n    })\r\n  },\r\n  data() {\r\n    return {\r\n      showWorkDetailsDialog: false,\r\n      selectedWorkDetails: '',\r\n      showAttachmentsDialog: false,\r\n      selectedAttachments: [],\r\n      leaveTypes: []\r\n    }\r\n  },\r\n  async mounted() {\r\n    await this.loadLeaveTypes()\r\n  },\r\n  methods: {\r\n    async loadLeaveTypes() {\r\n      try {\r\n        const response = await this.$http.get('/api/leave/leave-types')\r\n        this.leaveTypes = response.data\r\n        console.log('Leave types loaded:', this.leaveTypes)\r\n      } catch (error) {\r\n        console.error('Error loading leave types:', error)\r\n      }\r\n    },\r\n    getLeaveTypeColor(type) {\r\n      const leaveType = this.leaveTypes.find(lt => lt.value === type)\r\n      const color = leaveType?.color || '#6c757d'\r\n      console.log('Getting color for type:', type, 'found:', leaveType, 'color:', color)\r\n      return color\r\n    },\r\n    showWorkDetails(workDetails) {\r\n      this.selectedWorkDetails = workDetails\r\n      this.showWorkDetailsDialog = true\r\n    },\r\n\r\n    showAttachments(attachments) {\r\n      this.selectedAttachments = attachments || []\r\n      this.showAttachmentsDialog = true\r\n    },\r\n\r\n    downloadFile(fileName) {\r\n      this.$toast.add({\r\n        severity: 'success',\r\n        summary: 'ดาวน์โหลด',\r\n        detail: `กำลังดาวน์โหลด: ${fileName}`,\r\n        life: 3000\r\n      })\r\n    },\r\n\r\n    getFileIcon(fileName) {\r\n      const extension = fileName.split('.').pop()?.toLowerCase()\r\n      const icons = {\r\n        pdf: 'pi pi-file-pdf',\r\n        doc: 'pi pi-file-word',\r\n        docx: 'pi pi-file-word',\r\n        xls: 'pi pi-file-excel',\r\n        xlsx: 'pi pi-file-excel',\r\n        jpg: 'pi pi-image',\r\n        jpeg: 'pi pi-image',\r\n        png: 'pi pi-image',\r\n        gif: 'pi pi-image'\r\n      }\r\n      return icons[extension] || 'pi pi-file'\r\n    },\r\n\r\n    getFileType(fileName) {\r\n      const extension = fileName.split('.').pop()?.toLowerCase()\r\n      const types = {\r\n        pdf: 'PDF Document',\r\n        doc: 'Word Document',\r\n        docx: 'Word Document',\r\n        xls: 'Excel Spreadsheet',\r\n        xlsx: 'Excel Spreadsheet',\r\n        jpg: 'JPEG Image',\r\n        jpeg: 'JPEG Image',\r\n        png: 'PNG Image',\r\n        gif: 'GIF Image'\r\n      }\r\n      return types[extension] || 'Unknown File'\r\n    },\r\n    formatDateTime(datetime) {\r\n      if (!datetime) return '-'\r\n      return new Date(datetime).toLocaleString('th-TH', {\r\n        year: 'numeric',\r\n        month: 'short',\r\n        day: 'numeric',\r\n        hour: '2-digit',\r\n        minute: '2-digit'\r\n      })\r\n    },\r\n\r\n    formatDate(datetime) {\r\n      if (!datetime) return '-'\r\n      return new Date(datetime).toLocaleDateString('th-TH', {\r\n        day: '2-digit',\r\n        month: '2-digit'\r\n      })\r\n    },\r\n    calculateDays(startDateTime, endDateTime) {\r\n      if (!startDateTime || !endDateTime) return 0\r\n      const start = new Date(startDateTime)\r\n      const end = new Date(endDateTime)\r\n      const diffTime = Math.abs(end - start)\r\n      return Math.ceil(diffTime / (1000 * 60 * 60 * 24)) + 1\r\n    },\r\n    getLeaveTypeLabel(type) {\r\n      const types = {\r\n        sick: 'ลาป่วย',\r\n        personal: 'ลากิจ',\r\n        vacation: 'ลาพักร้อน'