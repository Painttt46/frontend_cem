{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useToast } from 'primevue/usetoast';\nimport { usePermissions } from '@/composables/usePermissions';\nexport default {\n  __name: 'ManagementView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const toast = useToast();\n    const {\n      loadPermissions,\n      hasAccess\n    } = usePermissions();\n    onMounted(() => {\n      loadPermissions();\n    });\n    const navigateTo = section => {\n      // Check user role\n      const userRole = localStorage.getItem('soc_role');\n      if (userRole !== 'admin' && userRole !== 'superadmin') {\n        toast.add({\n          severity: 'error',\n          summary: 'ไม่มีสิทธิ์เข้าถึง',\n          detail: 'คุณไม่มีสิทธิ์เข้าถึงส่วนจัดการนี้',\n          life: 3000\n        });\n        return;\n      }\n\n      // Navigate to management sections\n      switch (section) {\n        case 'dashboard':\n          router.push('/management/dashboard');\n          break;\n        case 'users':\n          router.push('/management/users');\n          break;\n        case 'leave-management':\n          router.push('/management/leave');\n          break;\n        case 'task-management':\n          router.push('/management/tasks');\n          break;\n        case 'settings':\n          router.push('/management/settings');\n          break;\n        case 'car-management':\n        case 'departments':\n        case 'reports':\n        case 'backup':\n          toast.add({\n            severity: 'info',\n            summary: 'กำลังพัฒนา',\n            detail: 'หน้านี้อยู่ระหว่างการพัฒนา',\n            life: 3000\n          });\n          break;\n        default:\n          toast.add({\n            severity: 'info',\n            summary: 'กำลังพัฒนา',\n            detail: 'ฟีเจอร์นี้อยู่ระหว่างการพัฒนา',\n            life: 3000\n          });\n      }\n    };\n    const __returned__ = {\n      router,\n      toast,\n      loadPermissions,\n      hasAccess,\n      navigateTo,\n      onMounted,\n      get useRouter() {\n        return useRouter;\n      },\n      get useToast() {\n        return useToast;\n      },\n      get usePermissions() {\n        return usePermissions;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["onMounted","useRouter","useToast","usePermissions","router","toast","loadPermissions","hasAccess","navigateTo","section","userRole","localStorage","getItem","add","severity","summary","detail","life","push"],"sources":["D:/frontend_cem/src/views/ManagementView.vue"],"sourcesContent":["<template>\n  <div class=\"management-container\">\n    <Toast />\n    \n    <!-- Header Card -->\n    <Card class=\"header-card mb-4\">\n      <template #content>\n        <div class=\"header-content\">\n          <div class=\"header-left\">\n            <div class=\"header-title\">\n              <i class=\"pi pi-cog header-icon\"></i>\n              <h1>ระบบจัดการ</h1>\n            </div>\n          </div>\n        </div>\n      </template>\n    </Card>\n\n    <!-- Management Grid -->\n    <div class=\"management-grid\">\n      <!-- Dashboard -->\n      <Card class=\"management-card\" @click=\"navigateTo('dashboard')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-chart-bar card-icon\"></i>\n              <Badge value=\"Active\" severity=\"success\" class=\"status-badge\" />\n            </div>\n            <h3>Dashboard</h3>\n            <p>รายงานสถิติและภาพรวมของระบบ</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- User Management -->\n      <Card v-if=\"hasAccess('/management/users')\" class=\"management-card\" @click=\"navigateTo('users')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-users card-icon\"></i>\n              <Badge value=\"Active\" severity=\"success\" class=\"status-badge\" />\n            </div>\n            <h3>จัดการผู้ใช้งาน</h3>\n            <p>เพิ่ม แก้ไข ลบ ผู้ใช้งาน และกำหนดสิทธิ์</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Leave Management -->\n      <Card v-if=\"hasAccess('/management/leave')\" class=\"management-card\" @click=\"navigateTo('leave-management')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-calendar-times card-icon\"></i>\n              <Badge value=\"Active\" severity=\"success\" class=\"status-badge\" />\n            </div>\n            <h3>จัดการการลางาน</h3>\n            <p>ตั้งค่าโควต้า อนุมัติการลา และรายงาน</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Car Management -->\n      <Card class=\"management-card\" @click=\"navigateTo('car-management')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-car card-icon\"></i>\n              <Badge value=\"Coming Soon\" severity=\"warning\" class=\"status-badge\" />\n            </div>\n            <h3>จัดการรถยนต์</h3>\n            <p>เพิ่มรถ กำหนดตารางใช้งาน และบำรุงรักษา</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Task Management -->\n      <Card v-if=\"hasAccess('/management/tasks')\" class=\"management-card\" @click=\"navigateTo('task-management')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-briefcase card-icon\"></i>\n              <Badge value=\"Active\" severity=\"success\" class=\"status-badge\" />\n            </div>\n            <h3>จัดการงาน</h3>\n            <p>สร้างงาน มอบหมายงาน และติดตามผล</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Department Management -->\n      <Card class=\"management-card\" @click=\"navigateTo('departments')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-building card-icon\"></i>\n              <Badge value=\"Coming Soon\" severity=\"warning\" class=\"status-badge\" />\n            </div>\n            <h3>จัดการแผนก</h3>\n            <p>เพิ่ม แก้ไข แผนกและตำแหน่งงาน</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Reports -->\n      <Card class=\"management-card\" @click=\"navigateTo('reports')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-chart-bar card-icon\"></i>\n              <Badge value=\"Coming Soon\" severity=\"warning\" class=\"status-badge\" />\n            </div>\n            <h3>รายงาน</h3>\n            <p>รายงานสถิติ การใช้งาน และประสิทธิภาพ</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- System Settings -->\n      <Card v-if=\"hasAccess('/management/settings')\" class=\"management-card\" @click=\"navigateTo('settings')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-cog card-icon\"></i>\n              <Badge value=\"Active\" severity=\"success\" class=\"status-badge\" />\n            </div>\n            <h3>ตั้งค่าระบบ</h3>\n            <p>การแจ้งเตือน การเชื่อมต่อ และการสำรองข้อมูล</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Backup & Restore -->\n      <Card class=\"management-card\" @click=\"navigateTo('backup')\">\n        <template #content>\n          <div class=\"card-content\">\n            <div class=\"card-header\">\n              <i class=\"pi pi-database card-icon\"></i>\n              <Badge value=\"Coming Soon\" severity=\"warning\" class=\"status-badge\" />\n            </div>\n            <h3>สำรองข้อมูล</h3>\n            <p>สำรองและกู้คืนข้อมูลระบบ</p>\n            <div class=\"card-footer\">\n              <i class=\"pi pi-arrow-right\"></i>\n            </div>\n          </div>\n        </template>\n      </Card>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { onMounted } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { useToast } from 'primevue/usetoast'\nimport { usePermissions } from '@/composables/usePermissions'\n\nconst router = useRouter()\nconst toast = useToast()\nconst { loadPermissions, hasAccess } = usePermissions()\n\nonMounted(() => {\n  loadPermissions()\n})\n\nconst navigateTo = (section) => {\n  // Check user role\n  const userRole = localStorage.getItem('soc_role')\n  \n  if (userRole !== 'admin' && userRole !== 'superadmin') {\n    toast.add({\n      severity: 'error',\n      summary: 'ไม่มีสิทธิ์เข้าถึง',\n      detail: 'คุณไม่มีสิทธิ์เข้าถึงส่วนจัดการนี้',\n      life: 3000\n    })\n    return\n  }\n\n  // Navigate to management sections\n  switch (section) {\n    case 'dashboard':\n      router.push('/management/dashboard')\n      break\n    case 'users':\n      router.push('/management/users')\n      break\n    case 'leave-management':\n      router.push('/management/leave')\n      break\n    case 'task-management':\n      router.push('/management/tasks')\n      break\n    case 'settings':\n      router.push('/management/settings')\n      break\n    case 'car-management':\n    case 'departments':\n    case 'reports':\n    case 'backup':\n      toast.add({\n        severity: 'info',\n        summary: 'กำลังพัฒนา',\n        detail: 'หน้านี้อยู่ระหว่างการพัฒนา',\n        life: 3000\n      })\n      break\n    default:\n      toast.add({\n        severity: 'info',\n        summary: 'กำลังพัฒนา',\n        detail: 'ฟีเจอร์นี้อยู่ระหว่างการพัฒนา',\n        life: 3000\n      })\n  }\n}\n</script>\n\n<style scoped>\n.management-container {\n  padding: 1rem;\n  max-width: 1400px;\n  margin: 0 auto;\n}\n\n/* Header Styles */\n.header-card {\n  background: linear-gradient(135deg, #4A90E2, #D73527);\n  color: white;\n  border: none;\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);\n}\n\n.header-content {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 1rem;\n}\n\n.header-left {\n  display: flex;\n  align-items: center;\n  gap: 1rem;\n}\n\n.header-title {\n  display: flex;\n  align-items: center;\n  gap: 0.75rem;\n}\n\n.header-icon {\n  font-size: 1.5rem;\n}\n\n.header-title h1 {\n  margin: 0;\n  font-size: 1.5rem;\n  font-weight: 600;\n}\n\n.header-stats {\n  display: flex;\n  align-items: center;\n}\n\n.system-count {\n  font-size: 0.875rem;\n  padding: 0.75rem 1.25rem;\n  background: rgba(255, 255, 255, 0.2);\n  color: white;\n  border: 1px solid rgba(255, 255, 255, 0.3);\n  border-radius: 20px;\n  white-space: nowrap;\n  display: inline-flex;\n  align-items: center;\n  gap: 0.5rem;\n}\n\n/* Grid Layout */\n.management-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(320px, 1fr));\n  gap: 1.5rem;\n}\n\n/* Card Styles */\n.management-card {\n  cursor: pointer;\n  transition: all 0.3s ease;\n  border: 1px solid #e9ecef;\n  border-radius: 12px;\n  overflow: hidden;\n  height: 100%;\n}\n\n.management-card:hover {\n  transform: translateY(-4px);\n  box-shadow: 0 12px 30px rgba(0, 0, 0, 0.15);\n  border-color: #4A90E2;\n}\n\n.card-content {\n  padding: 1.5rem;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  position: relative;\n}\n\n.card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  margin-bottom: 1rem;\n}\n\n.card-icon {\n  font-size: 2.5rem;\n  color: #4A90E2;\n  transition: color 0.3s ease;\n}\n\n.management-card:hover .card-icon {\n  color: #D73527;\n}\n\n.status-badge {\n  font-size: 0.7rem;\n  padding: 0.25rem 0.5rem;\n}\n\n.card-content h3 {\n  margin: 0 0 0.75rem 0;\n  color: #2c3e50;\n  font-size: 1.25rem;\n  font-weight: 600;\n}\n\n.card-content p {\n  margin: 0;\n  color: #6c757d;\n  font-size: 0.9rem;\n  line-height: 1.5;\n  flex: 1;\n}\n\n.card-footer {\n  display: flex;\n  justify-content: flex-end;\n  margin-top: 1rem;\n  padding-top: 1rem;\n  border-top: 1px solid #f8f9fa;\n}\n\n.card-footer i {\n  color: #4A90E2;\n  font-size: 1rem;\n  transition: transform 0.3s ease;\n}\n\n.management-card:hover .card-footer i {\n  transform: translateX(4px);\n  color: #D73527;\n}\n\n/* Mobile Responsive */\n@media (max-width: 768px) {\n  .management-container {\n    padding: 0.5rem;\n  }\n\n  .header-content {\n    flex-direction: column;\n    gap: 1rem;\n    text-align: center;\n  }\n\n  .header-left {\n    flex-direction: column;\n    gap: 0.5rem;\n  }\n\n  .header-stats {\n    margin-top: 0.5rem;\n  }\n\n  .management-grid {\n    grid-template-columns: 1fr;\n    gap: 1rem;\n  }\n  \n  .card-content {\n    padding: 1.25rem;\n  }\n  \n  .card-icon {\n    font-size: 2rem;\n  }\n\n  .card-content h3 {\n    font-size: 1.1rem;\n  }\n}\n\n@media (max-width: 480px) {\n  .management-container {\n    padding: 0.25rem;\n  }\n\n  .card-content {\n    padding: 1rem;\n  }\n\n  .card-icon {\n    font-size: 1.75rem;\n  }\n\n  .header-title h1 {\n    font-size: 1.25rem;\n  }\n}\n\n/* Animation */\n@keyframes fadeInUp {\n  from {\n    opacity: 0;\n    transform: translateY(20px);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n.management-card {\n  animation: fadeInUp 0.6s ease forwards;\n}\n\n.management-card:nth-child(1) { animation-delay: 0.1s; }\n.management-card:nth-child(2) { animation-delay: 0.2s; }\n.management-card:nth-child(3) { animation-delay: 0.3s; }\n.management-card:nth-child(4) { animation-delay: 0.4s; }\n.management-card:nth-child(5) { animation-delay: 0.5s; }\n.management-card:nth-child(6) { animation-delay: 0.6s; }\n.management-card:nth-child(7) { animation-delay: 0.7s; }\n.management-card:nth-child(8) { animation-delay: 0.8s; }\n</style>\n"],"mappings":";AAiLA,SAASA,SAAS,QAAQ,KAAI;AAC9B,SAASC,SAAS,QAAQ,YAAW;AACrC,SAASC,QAAQ,QAAQ,mBAAkB;AAC3C,SAASC,cAAc,QAAQ,8BAA6B;;;;;;;IAE5D,MAAMC,MAAM,GAAGH,SAAS,CAAC;IACzB,MAAMI,KAAK,GAAGH,QAAQ,CAAC;IACvB,MAAM;MAAEI,eAAe;MAAEC;IAAU,CAAC,GAAGJ,cAAc,CAAC;IAEtDH,SAAS,CAAC,MAAM;MACdM,eAAe,CAAC;IAClB,CAAC;IAED,MAAME,UAAU,GAAIC,OAAO,IAAK;MAC9B;MACA,MAAMC,QAAQ,GAAGC,YAAY,CAACC,OAAO,CAAC,UAAU;MAEhD,IAAIF,QAAQ,KAAK,OAAO,IAAIA,QAAQ,KAAK,YAAY,EAAE;QACrDL,KAAK,CAACQ,GAAG,CAAC;UACRC,QAAQ,EAAE,OAAO;UACjBC,OAAO,EAAE,oBAAoB;UAC7BC,MAAM,EAAE,oCAAoC;UAC5CC,IAAI,EAAE;QACR,CAAC;QACD;MACF;;MAEA;MACA,QAAQR,OAAO;QACb,KAAK,WAAW;UACdL,MAAM,CAACc,IAAI,CAAC,uBAAuB;UACnC;QACF,KAAK,OAAO;UACVd,MAAM,CAACc,IAAI,CAAC,mBAAmB;UAC/B;QACF,KAAK,kBAAkB;UACrBd,MAAM,CAACc,IAAI,CAAC,mBAAmB;UAC/B;QACF,KAAK,iBAAiB;UACpBd,MAAM,CAACc,IAAI,CAAC,mBAAmB;UAC/B;QACF,KAAK,UAAU;UACbd,MAAM,CAACc,IAAI,CAAC,sBAAsB;UAClC;QACF,KAAK,gBAAgB;QACrB,KAAK,aAAa;QAClB,KAAK,SAAS;QACd,KAAK,QAAQ;UACXb,KAAK,CAACQ,GAAG,CAAC;YACRC,QAAQ,EAAE,MAAM;YAChBC