ption,\n          category: this.formData.category,\n          files: uploadedFiles\n        };\n        await this.$http.post('/api/tasks', taskData);\n        this.$toast.add({\n          severity: 'success',\n          summary: '‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à',\n          detail: '‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏á‡∏≤‡∏ô‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß',\n          life: 3000\n        });\n\n        // Emit to parent component\n        this.$emit('task-added');\n\n        // Dispatch global event for real-time update\n        window.dispatchEvent(new CustomEvent('taskUpdated'));\n        this.resetForm();\n      } catch (error) {\n        const errorMessage = error.response?.data?.error || error.message || '‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏á‡∏≤‡∏ô‡πÑ‡∏î‡πâ';\n        this.$toast.add({\n          severity: 'error',\n          summary: '‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î',\n          detail: errorMessage,\n          life: 5000\n        });\n      }\n    },\n    resetForm() {\n      this.formData = {\n        taskName: '',\n        soNumber: '',\n        contractNumber: '',\n        saleOwner: '',\n        projectStartDate: null,\n        projectEndDate: null,\n        description: '',\n        category: '‡∏á‡∏≤‡∏ô‡∏ó‡∏±‡πà‡∏ß‡πÑ‡∏õ',\n        files: []\n      };\n      const fileInput = document.getElementById('fileUpload');\n      if (fileInput) fileInput.value = '';\n    }\n  }\n};","map":{"version":3,"names":["axios","name","created","$http","create","baseURL","data","formData","taskName","soNumber","contractNumber","saleOwner","projectStartDate","projectEndDate","description","category","files","categoryOptions","mounted","loadCategories","methods","savedCategories","localStorage","getItem","categories","JSON","parse","map","cat","icon","getDefaultIcon","value","label","iconMap","getCategoryIcon","find","getCategoryLabel","handleFileUpload","event","Array","from","target","removeFile","index","splice","uploadFiles","length","FormData","forEach","file"