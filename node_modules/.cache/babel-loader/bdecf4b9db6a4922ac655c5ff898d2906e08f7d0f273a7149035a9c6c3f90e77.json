{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { useToast } from 'primevue/usetoast';\nimport axios from 'axios';\nexport default {\n  __name: 'TaskManagement',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const toast = useToast();\n    const http = axios.create({\n      baseURL: 'http://localhost:3001'\n    });\n\n    // Data\n    const showCategoriesDialog = ref(false);\n    const showStatusDialog = ref(false);\n    const newCategory = ref('');\n    const newStatus = ref('');\n    const newCategoryIcon = ref('');\n    const newStatusIcon = ref('');\n\n    // Icon Options\n    const categoryIcons = ref([{\n      label: 'üíº',\n      value: 'emoji:üíº'\n    }, {\n      label: 'üíª',\n      value: 'emoji:üíª'\n    }, {\n      label: 'üîß',\n      value: 'emoji:üîß'\n    }, {\n      label: 'üë•',\n      value: 'emoji:üë•'\n    }, {\n      label: 'üìö',\n      value: 'emoji:üìö'\n    }, {\n      label: 'üîç',\n      value: 'emoji:üîç'\n    }, {\n      label: 'üìù',\n      value: 'emoji:üìù'\n    }, {\n      label: 'üë§',\n      value: 'emoji:üë§'\n    }, {\n      label: 'üí∞',\n      value: 'emoji:üí∞'\n    }, {\n      label: 'üì¢',\n      value: 'emoji:üì¢'\n    }, {\n      label: 'üè†',\n      value: 'emoji:üè†'\n    }, {\n      label: 'üöó',\n      value: 'emoji:üöó'\n    }, {\n      label: 'üì±',\n      value: 'emoji:üì±'\n    }, {\n      label: 'üéØ',\n      value: 'emoji:üéØ'\n    }, {\n      label: 'üìä',\n      value: 'emoji:üìä'\n    }, {\n      label: 'üé®',\n      value: 'emoji:üé®'\n    }, {\n      label: 'üîí',\n      value: 'emoji:üîí'\n    }, {\n      label: 'üåê',\n      value: 'emoji:üåê'\n    }, {\n      label: 'üìà',\n      value: 'emoji:üìà'\n    }, {\n      label: 'üèÜ',\n      value: 'emoji:üèÜ'\n    }]);\n    const statusIcons = ref([{\n      label: '‚è≥',\n      value: 'emoji:‚è≥'\n    }, {\n      label: 'üîÑ',\n      value: 'emoji:üîÑ'\n    }, {\n      label: '‚úÖ',\n      value: 'emoji:‚úÖ'\n    }, {\n      label: '‚è∏Ô∏è',\n      value: 'emoji:‚è∏Ô∏è'\n    }, {\n      label: '‚ùå',\n      value: 'emoji:‚ùå'\n    }, {\n      label: '‚ö†Ô∏è',\n      value: 'emoji:‚ö†Ô∏è'\n    }, {\n      label: 'üìù',\n      value: 'emoji:üìù'\n    }, {\n      label: 'üöÄ',\n      value: 'emoji:üöÄ'\n    }, {\n      label: 'üéØ',\n      value: 'emoji:üéØ'\n    }, {\n      label: 'üí°',\n      value: 'emoji:üí°'\n    }, {\n      label: 'üî•',\n      value: 'emoji:üî•'\n    }, {\n      label: '‚ö°',\n      value: 'emoji:‚ö°'\n    }, {\n      label: 'üåü',\n      value: 'emoji:üåü'\n    }, {\n      label: 'üéâ',\n      value: 'emoji:üéâ'\n    }, {\n      label: 'üèÅ',\n      value: 'emoji:üèÅ'\n    }, {\n      label: 'üîî',\n      value: 'emoji:üîî'\n    }, {\n      label: 'üìå',\n      value: 'emoji:üìå'\n    }, {\n      label: 'üí§',\n      value: 'emoji:üí§'\n    }, {\n      label: 'ü§î',\n      value: 'emoji:ü§î'\n    }, {\n      label: 'üîí',\n      value: 'emoji:üîí'\n    }]);\n\n    // Categories and Status\n    const categories = ref([]);\n    const workStatuses = ref([]);\n\n    // Methods\n    const loadCategories = async () => {\n      try {\n        const response = await http.get('/api/settings/categories');\n        categories.value = response.data;\n      } catch (error) {\n        console.error(error);\n      }\n    };\n    const loadStatuses = async () => {\n      try {\n        const response = await http.get('/api/settings/statuses');\n        workStatuses.value = response.data;\n      } catch (error) {\n        console.error(error);\n      }\n    };\n    const addCategory = async () => {\n      if (newCategory.value.trim() && newCategoryIcon.value) {\n        try {\n          const emoji = newCategoryIcon.value.replace('emoji:', '');\n          await http.post('/api/settings/categories', {\n            label: `${emoji} ${newCategory.value.trim()}`,\n            value: newCategory.value.trim(),\n            icon: newCategoryIcon.value\n          });\n          newCategory.value = '';\n          newCategoryIcon.value = '';\n          await loadCategories();\n          window.dispatchEvent(new CustomEvent('categoriesUpdated'));\n          toast.add({\n            severity: 'success',\n            summary: '‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à',\n            detail: '‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢',\n            life: 3000\n          });\n        } catch (error) {\n          toast.add({\n            severity: 'error',\n            summary: '‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î',\n            detail: error.response?.data?.error || '‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡πÑ‡∏î‡πâ',\n            life: 3000\n          });\n        }\n      }\n    };\n    const removeCategory = async categoryValue => {\n      try {\n        await http.delete(`/api/settings/categories/${categoryValue}`);\n        await loadCategories();\n        window.dispatchEvent(new CustomEvent('categoriesUpdated'));\n        toast.add({\n          severity: 'success',\n          summary: '‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à',\n          detail: '‡∏•‡∏ö‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢',\n          life: 3000\n        });\n      } catch (error) {\n        toast.add({\n          severity: 'error',\n          summary: '‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î',\n          detail: '‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏•‡∏ö‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡πÑ‡∏î‡πâ',\n          life: 3000\n        });\n      }\n    };\n    const moveCategoryUp = async index => {\n      if (index > 0) {\n        const temp = categories.value[index];\n        categories.value[index] = categories.value[index - 1];\n        categories.value[index - 1] = temp;\n        await http.put('/api/settings/categories/reorder', {\n          categories: categories.value\n        });\n        window.dispatchEvent(new CustomEvent('categoriesUpdated'));\n      }\n    };\n    const moveCategoryDown = async index => {\n      if (index < categories.value.length - 1) {\n        const temp = categories.value[index];\n        categories.value[index] = categories.value[index + 1];\n        categories.value[index + 1] = temp;\n        await http.put('/api/settings/categories/reorder', {\n          categories: categories.value\n        });\n        window.dispatchEvent(new CustomEvent('categoriesUpdated'));\n      }\n    };\n    const addStatus = async () => {\n      if (newStatus.value.trim() && newStatusIcon.value) {\n        try {\n          const emoji = newStatusIcon.value.replace('emoji:', '');\n          await http.post('/api/settings/statuses', {\n            label: `${emoji} ${newStatus.value.trim()}`,\n            value: newStatus.value.trim().toLowerCase().replace(/\\s+/g, '_'),\n            icon: newStatusIcon.value\n          });\n          newStatus.value = '';\n          newStatusIcon.value = '';\n          await loadStatuses();\n          window.dispatchEvent(new CustomEvent('statusesUpdated'));\n          toast.add({\n            severity: 'success',\n            summary: '‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à',\n            detail: '‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢',\n            life: 3000\n          });\n        } catch (error) {\n          toast.add({\n            severity: 'error',\n            summary: '‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î',\n            detail: error.response?.data?.error || '‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÑ‡∏î‡πâ',\n            life: 3000\n          });\n        }\n      }\n    };\n    const removeStatus = async statusValue => {\n      try {\n        await http.delete(`/api/settings/statuses/${statusValue}`);\n        await loadStatuses();\n        window.dispatchEvent(new CustomEvent('statusesUpdated'));\n        toast.add({\n          severity: 'success',\n          summary: '‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à',\n          detail: '‡∏•‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢',\n          life: 3000\n        });\n      } catch (error) {\n        toast.add({\n          severity: 'error',\n          summary: '‡∏Ç‡πâ‡∏≠‡∏ú‡∏¥‡∏î‡∏û‡∏•‡∏≤‡∏î',\n          detail: '‡πÑ‡∏°‡πà‡∏™‡∏≤‡∏°‡∏≤‡∏£‡∏ñ‡∏•‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡πÑ‡∏î‡πâ',\n          life: 3000\n        });\n      }\n    };\n    const moveStatusUp = async index => {\n      if (index > 0) {\n        const temp = workStatuses.value[index];\n        workStatuses.value[index] = workStatuses.value[index - 1];\n        workStatuses.value[index - 1] = temp;\n        await http.put('/api/settings/statuses/reorder', {\n          statuses: workStatuses.value\n        });\n        window.dispatchEvent(new CustomEvent('statusesUpdated'));\n      }\n    };\n    const moveStatusDown = async index => {\n      if (index < workStatuses.value.length - 1) {\n        const temp = workStatuses.value[index];\n        workStatuses.value[index] = workStatuses.value[index + 1];\n        workStatuses.value[index + 1] = temp;\n        await http.put('/api/settings/statuses/reorder', {\n          statuses: workStatuses.value\n        });\n        window.dispatchEvent(new CustomEvent('statusesUpdated'));\n      }\n    };\n    const closeCategoryDialog = () => {\n      showCategoriesDialog.value = false;\n      newCategory.value = '';\n      newCategoryIcon.value = '';\n    };\n    const closeStatusDialog = () => {\n      showStatusDialog.value = false;\n      newStatus.value = '';\n      newStatusIcon.value = '';\n    };\n    onMounted(() => {\n      loadCategories();\n      loadStatuses();\n    });\n    const __returned__ = {\n      toast,\n      http,\n      showCategoriesDialog,\n      showStatusDialog,\n      newCategory,\n      newStatus,\n      newCategoryIcon,\n      newStatusIcon,\n      categoryIcons,\n      statusIcons,\n      categories,\n      workStatuses,\n      loadCategories,\n      loadStatuses,\n      addCategory,\n      removeCategory,\n      moveCategoryUp,\n      moveCategoryDown,\n      addStatus,\n      removeStatus,\n      moveStatusUp,\n      moveStatusDown,\n      closeCategoryDialog,\n      closeStatusDialog,\n      ref,\n      onMounted,\n      get useToast() {\n        return useToast;\n      },\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useToast","axios","toast","http","create","baseURL","showCategoriesDialog","showStatusDialog","newCategory","newStatus","newCategoryIcon","newStatusIcon","categoryIcons","label","value","statusIcons","categories","workStatuses","loadCategories","response","get","data","error","console","loadStatuses","addCategory","trim","emoji","replace","post","icon","window","dispatchEvent","CustomEvent","add","severity","summary","detail","life","removeCategory","categoryValue","delete","moveCategoryUp","index","temp","put","moveCategoryDown","length","addStatus","toLowerCase","removeStatus","statusValue","moveStatusUp","statuses","moveStatusDown","closeCategoryDialog","closeStatusDialog"],"sources":["D:/FULLACC/frontend_cem-main/src/views/management/TaskManagement.vue"],"sourcesContent":["<template>\n  <div class=\"task-management\">\n    <Toast />\n    \n    <!-- Header Card -->\n    <Card class=\"header-card mb-4\">\n      <template #content>\n        <div class=\"header-content\">\n          <div class=\"header-left\">\n            <Button icon=\"pi pi-arrow-left\" @click=\"$router.go(-1)\" text class=\"back-btn\" />\n            <div class=\"header-title\">\n              <i class=\"pi pi-briefcase header-icon\"></i>\n              <h1>‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏á‡∏≤‡∏ô</h1>\n            </div>\n          </div>\n          <div class=\"header-stats\">\n            <div class=\"task-count\">\n              ‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà: {{ categories.length }} | ‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: {{ workStatuses.length }}\n            </div>\n          </div>\n        </div>\n      </template>\n    </Card>\n\n    <!-- Action Cards -->\n    <div class=\"action-grid\">\n      <!-- All Projects Card -->\n      <Card class=\"action-card\" @click=\"$router.push('/management/projects')\">\n        <template #content>\n          <div class=\"action-content\">\n            <div class=\"action-header\">\n              <i class=\"pi pi-folder action-icon\"></i>\n              <h3>‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏á‡∏≤‡∏ô</h3>\n            </div>\n            <p>‡∏î‡∏π‡πÅ‡∏•‡∏∞‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£‡∏á‡∏≤‡∏ô‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î</p>\n            <div class=\"action-stats\">\n              <span class=\"stat-label\">‡∏î‡∏π‡∏£‡∏≤‡∏¢‡∏•‡∏∞‡πÄ‡∏≠‡∏µ‡∏¢‡∏î</span>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Daily Work Card -->\n      <Card class=\"action-card\" @click=\"$router.push('/management/daily-work')\">\n        <template #content>\n          <div class=\"action-content\">\n            <div class=\"action-header\">\n              <i class=\"pi pi-calendar action-icon\"></i>\n              <h3>‡∏á‡∏≤‡∏ô‡∏£‡∏≤‡∏¢‡∏ß‡∏±‡∏ô</h3>\n            </div>\n            <p>‡∏î‡∏π‡πÅ‡∏•‡∏∞‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏á‡∏≤‡∏ô‡∏£‡∏≤‡∏¢‡∏ß‡∏±‡∏ô</p>\n            <div class=\"action-stats\">\n              <span class=\"stat-label\">‡∏î‡∏π‡∏£‡∏≤‡∏¢‡∏•‡∏∞‡πÄ‡∏≠‡∏µ‡∏¢‡∏î</span>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Categories Management Card -->\n      <Card class=\"action-card\" @click=\"showCategoriesDialog = true\">\n        <template #content>\n          <div class=\"action-content\">\n            <div class=\"action-header\">\n              <i class=\"pi pi-tags action-icon\"></i>\n              <h3>‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡∏á‡∏≤‡∏ô</h3>\n            </div>\n            <p>‡πÄ‡∏û‡∏¥‡πà‡∏° ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç ‡∏´‡∏£‡∏∑‡∏≠‡∏•‡∏ö‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡∏á‡∏≤‡∏ô</p>\n            <div class=\"action-stats\">\n              <span class=\"stat-number\">{{ categories.length }}</span>\n              <span class=\"stat-label\">‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà</span>\n            </div>\n          </div>\n        </template>\n      </Card>\n\n      <!-- Status Management Card -->\n      <Card class=\"action-card\" @click=\"showStatusDialog = true\">\n        <template #content>\n          <div class=\"action-content\">\n            <div class=\"action-header\">\n              <i class=\"pi pi-flag action-icon\"></i>\n              <h3>‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏á‡∏≤‡∏ô</h3>\n            </div>\n            <p>‡πÄ‡∏û‡∏¥‡πà‡∏° ‡πÅ‡∏Å‡πâ‡πÑ‡∏Ç ‡∏´‡∏£‡∏∑‡∏≠‡∏•‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏á‡∏≤‡∏ô</p>\n            <div class=\"action-stats\">\n              <span class=\"stat-number\">{{ workStatuses.length }}</span>\n              <span class=\"stat-label\">‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞</span>\n            </div>\n          </div>\n        </template>\n      </Card>\n    </div>\n\n    <!-- Manage Categories Dialog -->\n    <Dialog v-model:visible=\"showCategoriesDialog\" header=\"‡∏à‡∏±‡∏î‡∏Å‡∏≤‡∏£‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡∏á‡∏≤‡∏ô\" \n            :style=\"{width: '600px'}\" modal :draggable=\"false\" position=\"center\">\n      <div class=\"dialog-content\">\n        <div class=\"add-section mb-4\">\n          <div class=\"flex flex-column gap-3\">\n            <div class=\"flex gap-2\">\n              <InputText v-model=\"newCategory\" placeholder=\"‡∏ä‡∏∑‡πà‡∏≠‡∏´‡∏°‡∏ß‡∏î‡∏´‡∏°‡∏π‡πà‡πÉ‡∏´‡∏°‡πà\" class=\"flex-1\" />\n              <Dropdown v-model=\"newCategoryIcon\" :options=\"categoryIcons\" \n                        optionLabel=\"label\" optionValue=\"value\" placeholder=\"‡πÄ‡∏•‡∏∑‡∏≠‡∏Å Icon\" \n                        class=\"icon-dropdown\">\n                <template #value=\"slotProps\">\n                  <div v-if=\"slotProps.value\" class=\"icon-display\">\n                    <span v-if=\"slotProps.value.startsWith('emoji:')\" class=\"emoji\">{{ slotProps.value.replace('emoji:', '') }}</span>\n                    <i v-else :class=\"slotProps.value\"></i>\n         